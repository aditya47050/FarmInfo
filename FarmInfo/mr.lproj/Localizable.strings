"pests_title" = "कीड";

"pest_Carrot Fly_name" = "गाजर माशी";
"pest_Carrot Fly_body" = "गाजर माशी (Psila rosae) एक काळ्या रंगाच्या शरीराची आणि नारिंगी डोक्याची कीड असून ती गाजर, पर्स्निप, सेलरी व इतर Apiaceae वनस्पतींना प्रभावित करते...";
"pest_Carrot Fly_symptoms" = "मुळात जंग ऋष्ठे, पर्ण पिवळटपणा व झुडप झोपाळं, फाट पडणारी मुळे, विषबाधा.";
"pest_Carrot Fly_treatment" = "जाळीनेटने झुडप झाकून अंडी पाडणे रोखावे, पिकफेरी करावी, प्रभावित झाडे काढून टाकावीत.";

"pest_Aphids_name" = "एफिड्स";
"pest_Aphids_body" = "एफिड्स लहान, मऊ शरीराच्या कीड आहेत, ज्या पाने व कांडातील रस घेतात...";
"pest_Aphids_symptoms" = "पान वाकणे, पिवळट पडणे, चिकट हनीड्यू, अंडी व अळी.";
"pest_Aphids_treatment" = "लॅडीबग्स व लेस-विंग्स सोबत आणा, जोराचा पाणी फवारून कीटक काढा, नीम तेल/इनस औद्य.";

"pest_Pepper Weevil_name" = "मिरची व्युल";
"pest_Pepper Weevil_body" = "मिरची व्युल (Anthonomus eugenii) एक लहान कोवळ्या रंगाची कीट आहे...";
"pest_Pepper Weevil_symptoms" = "फुल व फळ पूर्वी पडणे, मणीवर पिवळट व उरल सुके.";
"pest_Pepper Weevil_treatment" = "पेहरोमोन ट्रॅप व जैविक नियंत्रक. प्रभावित फळे काढा.";

"pest_Thrips_name" = "थ्रिप्स";
"pest_Thrips_body" = "थ्रिप्स लहान, सडपातळ कीट आहेत...";
"pest_Thrips_symptoms" = "पानांवर चांदीसर डाग, वाढीत विकृती, काळे थुके.";
"pest_Thrips_treatment" = "लेस-विंग्स व पायरट बग सोबत, चिकट ट्रॅप, नीम तेल.";

"pest_Bean Beetle_name" = "हरभरा व्युल";
"pest_Bean Beetle_body" = "हरभरा व्युल (cowpea weevil) शेंगीवर अंडी घालते...";
"pest_Bean Beetle_symptoms" = "शेंग गोल, आत अळ्या.";
"pest_Bean Beetle_treatment" = "हवा-रोधक डब्यात साठवा किंवा गोठव십시오.";

"pest_Spider Mites_name" = "स्पायडर माईट";
"pest_Spider Mites_body" = "लहान अ‍ॅरॅक्निड्स, रस शोषतात...";
"pest_Spider Mites_symptoms" = "पानांवर डाग, जाळी.";
"pest_Spider Mites_treatment" = "पाणी फवारले, नैसर्गिक शत्रू, नीम तेल.";

"pest_Onion Fly_name" = "कांद्याची माशी";
"pest_Onion Fly_body" = "कांद्याच्या कंदांवर अंडी घालणारी माशी...";
"pest_Onion Fly_symptoms" = "पांढऱ्या, तपकिरी, कंद सडणे.";
"pest_Onion Fly_treatment" = "जाळीनेट, फळीफेर, नॅमॅटोډ्स.";

"pest_Tomato Hornworm_name" = "टोमॅटो हॉर्नवॉर्म";
"pest_Tomato Hornworm_body" = "मोठी हिरवी अळी, शेवटी एक शिंगासारखा भुजा...";
"pest_Tomato Hornworm_symptoms" = "पाने खातात, फुली पडतात, थुका.";
