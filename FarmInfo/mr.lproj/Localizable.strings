//MARK: Vegetables
"vegetable_4_name" = "गाजर";
"vegetable_4_description" = "एक बहुमुखी मुळभाजी, सहसा नारिंगी पण जांभळा, लाल, पिवळा आणि पांढर्‍या रंगातही उपलब्ध. गाजर त्याच्या गोड चवी आणि उच्च बीटा-कॅरोटीन प्रमाणासाठी ओळखली जाते.";
"vegetable_4_healthBenefits" = "बीटा-कॅरोटीनने समृद्ध, गाजर डोळ्यांचे आरोग्य सुधारतात, रोगप्रतिकारक शक्ती वाढवतात आणि त्वचेचा आरोग्य राखतात.";
"vegetable_4_growingDescription" = "बियाणे काही आठवड्यांत अंकुरित होतात. पाणी देताना पाने ओलसर होऊ नयेत कारण सूर्यप्रकाशाने होणारी छिद्रे पडण्याची शक्यता असते.";
"vegetable_4_harvestDescription" = "पृथ्वीवर उभा राहणारा भाग क्वार्टरच्या व्यासासारखा दिसताच गाजर काढा. कंद जंतू होऊ नयेत म्हणून काळजीपूर्वक खोदा.";
"vegetable_4_seedDepth" = "1/4-1/2\" (6-13 मिमी)";
"vegetable_4_watering" = "मध्यम, माती ओलसर ठेवा";
"vegetable_4_light" = "पूर्ण सूर्यप्रकाश";
"vegetable_4_goodCompanions" = "कांदा, लेक्स, कोशिंबीर";
"vegetable_4_badCompanions" = "सोआ, पार्सनिप्स";

