//MARK: Pests (Hindi)
"pests_title" = "कीट";

"pest_Carrot Fly_name" = "गाजर मक्खी";
"pest_Carrot Fly_body" = "गाजर मक्खी (Psila rosae) एक काले रंग की कीट है, नारंगी सिर और टांगों के साथ, जो गाजर, पार्सनिप, अजमोद और Apiaceae पौधों को नुकसान पहुंचाती है...";
"pest_Carrot Fly_symptoms" = "जड़ में जंग जैसे सुरंगें, पत्ते पीले या झुरझुरीदार हो जाना, जड़ों में दरारें।";
"pest_Carrot Fly_treatment" = "सूक्ष्म जाली से पौधों को ढकें, फसल चक्रीकरण करें, प्रभावित पौधों को तुरंत नष्ट करें।";

"pest_Aphids_name" = "एफ़िड्स";
"pest_Aphids_body" = "एफ़िड्स छोटे, नरम शरीर वाले कीट होते हैं जो रस चूसते हैं...";
"pest_Aphids_symptoms" = "पत्ते मुड़ना या पीलापन, चिपचिपा हनीड्यू, काली फफूंदी।";
"pest_Aphids_treatment" = "लेडीबग व लेस-विंग्स लाएं, पानी की तेज धार से कीट हटाएं, नीम तेल/कीटकनाशक साबुन का उपयोग करें।";

"pest_Pepper Weevil_name" = "मिर्च चूहा";
"pest_Pepper Weevil_body" = "मिर्च चूहा (Anthonomus eugenii) एक छोटा अंधेरा रंग वाला बीटल है...";
"pest_Pepper Weevil_symptoms" = "फूल या फल का समय से पहले गिरना, तनों और कैलिक्स का पीलापन...";
"pest_Pepper Weevil_treatment" = "पेहरोमोन ट्रैप और जैविक नियंत्रण विधियाँ अपनाएँ।";

"pest_Thrips_name" = "थ्रिप्स";
"pest_Thrips_body" = "थ्रिप्स लम्बे पतले कीट हैं जो पत्तों/फूल की कोशिकाएं चूसते हैं...";
"pest_Thrips_symptoms" = "पत्तों पर चांदी जैसा प्रभाव, विकृत विकास, काले मल के दाग।";
"pest_Thrips_treatment" = "लेज-विंग्स व मिनट पायरेट बग का प्रयोग करें, चिपचिपे ट्रैप और नीम तेल का उपयोग करें।";

"pest_Bean Beetle_name" = "बीन बीटल";
"pest_Bean Beetle_body" = "हरभरे की फली पर अंडे देती है, अंडे से निकलने वाले कीट बीजों को नुकसान पहुँचाते हैं...";
"pest_Bean Beetle_symptoms" = "भृत बीन्स, अंडों/लार्वा के छिद्र।";
"pest_Bean Beetle_treatment" = "हवादार कंटेनर या फ्रीजर में संग्रहित करें।";

"pest_Spider Mites_name" = "स्पाइडर माइट्स";
"pest_Spider Mites_body" = "बहुत छोटे मकड़ी जैसे कीट जो रस चूसते हैं...";
"pest_Spider Mites_symptoms" = "पत्तों पर डाग, सूखा पत्ता, जाल।";
"pest_Spider Mites_treatment" = "पानी फैंकें, प्राकृतिक शिकारी जोड़ें, नीम तेल लगाएं।";

"pest_Onion Fly_name" = "प्याज़ मक्खी";
"pest_Onion Fly_body" = "प्याज़, लहसुन, लीक जैसी Allium फसलों में अंडे लगाने वाली मक्खी...";
