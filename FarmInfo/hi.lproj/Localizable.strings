//MARK: Vegetables
"vegetable_4_name" = "गाजर";
"vegetable_4_description" = "एक बहुमुखी कंदसब्ज़ी जो आमतौर पर नारंगी होती है, लेकिन जामुनी, लाल, पीली और सफ़ेद रंगों में भी उपलब्ध है। गाजर अपनी मीठी स्वाद और उच्च बीटा-कैरोटीन सामग्री के लिए जानी जाती है।";
"vegetable_4_healthBenefits" = "बीटा-कैरोटीन से भरपूर, गाजर आंखों के स्वास्थ्य को बढ़ावा देती है, प्रतिरक्षा प्रणाली को मजबूत करती है, और त्वचा को स्वस्थ बनाती है।";
"vegetable_4_growingDescription" = "बीज कुछ हफ़्तों में अंकुरित हो जाते हैं। पानी देते समय पत्तियों को गीला होने से बचाएं—सूर्य प्रकाश उन्हें नुकसान पहुंचा सकता है।";
"vegetable_4_harvestDescription" = "जब जमीन से थोड़ी हिस्सा क्वार्टर के व्यास जैसा उभर आए, तब गाजर को सावधानीपूर्वक खोदें ताकि कंद को क्षति न पहुंचे।";
"vegetable_4_seedDepth" = "1/4-1/2\" (6-13 मिमी)";
"vegetable_4_watering" = "मध्यम, मिट्टी को नम रखें";
"vegetable_4_light" = "पूरी धूप";
"vegetable_4_goodCompanions" = "प्याज, हरा प्याज, सलाद";
"vegetable_4_badCompanions" = "सोआ, पार्सनिप्स";


