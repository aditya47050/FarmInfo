"vegetable_7_name" = "तमाम मिर्च";
"vegetable_7_description" = "एक तीखी और शानदार सब्ज़ी जो किसी भी व्यंजन में मसाला जोड़ती है। मिर्चें हल्के से तीव्र तक के दर्जों में आती हैं और विटामिन C तथा कैप्सैसिन से समृद्ध होती हैं।";
"vegetable_7_healthBenefits" = "कैप्सैसिन दर्द को कम कर सकता है, पाचन में मदद करता है, और चयापचय को बढ़ावा देता है।";
"vegetable_7_growingDescription" = "मिट्टी को बराबर नम रखें। मिर्च के पौधे को प्रति सप्ताह कम से कम 1.5 इंच पानी दें। नमी बनाए रखने के लिए उपर मल्च लगाएं।";
"vegetable_7_harvestDescription" = "जब मिर्च वांछित आकार तक पहुंच जाए, तब उसे काटें—जितनी बार आप काटेंगे उतनी अधिक पैदावार होगी।";
"vegetable_7_seedDepth" = "1/4 इंच (6 मिमी)";
"vegetable_7_watering" = "मध्यम से अधिक, प्रति सप्ताह 1.5 इंच पानी";
"vegetable_7_light" = "पूरा सूर्य";
"vegetable_7_goodCompanions" = "तुलसी, गाजर, प्याज";
"vegetable_7_badCompanions" = "बीन्स";

"vegetable_8_name" = "हरी फली";
"vegetable_8_description" = "बगीचे की पसंदीदा दूसरी सबसे लोकप्रिय फली है (टमाटर के बाद)। हरी सेम कुरकुरे, कोमल और फाइबर से भरपूर होते हैं, स्टर-फ्राय या साइड डिश के रूप में उत्तम।";
"vegetable_8_healthBenefits" = "फाइबर और विटामिन से समृद्ध, पाचन और प्रतिरक्षा स्वास्थ्य का समर्थन करते हैं।";
"vegetable_8_growingDescription" = "क्षेत्र को साफ रखें और जंगली खरपतवार हटाएं—वे पोषक तत्व चुरा सकते हैं। हर दिन पानी दें। मल्च लगाने से नमी बनी रहती है।";
"vegetable_8_harvestDescription" = "जब हरी फली लंबी और सख्त हो जाए, तो उसे काट लें—जितनी फसल आप उठाएंगे, उतनी अधिक उत्पादन होगी।";
"vegetable_8_seedDepth" = "1 इंच (2.5 सेमी)";
"vegetable_8_watering" = "नियमित, मिट्टी को नम रखें";
"vegetable_8_light" = "पूरा सूर्य";
"vegetable_8_goodCompanions" = "मक्का, खीरा, स्ट्रॉबेरी";
"vegetable_8_badCompanions" = "प्याज, लहसुन";

"vegetable_9_name" = "प्याज़";
"vegetable_9_description" = "एक तीखी और स्वादिष्ट मूलभाजी जो सूप, स्ट्यू, सलाद और सॉते जैसे कई व्यंजनों में उपयोग होती है।";
"vegetable_9_healthBenefits" = "प्रभावी एंटीऑक्सिडेंट्स और सूजन-रोधी यौगिकों से भरपूर, हृदय स्वास्थ्य को बढ़ावा देता है।";
"vegetable_9_growingDescription" = "प्याज़ चुनते समय, वे पूर्ण, मजबूत और तनेदार होने चाहिए। एक छोटा गड्ढा खोदें और प्याज़ की नोक बाहर रहे।";
"vegetable_9_harvestDescription" = "प्रारंभ में, प्याज़ को सावधानीपूर्वक निकालें।";
"vegetable_9_seedDepth" = "1/2 इंच (13 मिमी)";
"vegetable_9_watering" = "हल्का लेकिन नियमित सिंचन";
"vegetable_9_light" = "पूरा सूर्य";
"vegetable_9_goodCompanions" = "गाजर, सलाद, चुकंदर";
"vegetable_9_badCompanions" = "बीन्स, मटर";

"vegetable_10_name" = "टमाटर";
"vegetable_10_description" = "एक रसदार और स्वादिष्ट फल जो अक्सर सब्ज़ी समझा जाता है। यह कई प्रकारों और रंगों में आता है और लाइकोपीन से समृद्ध है।";
"vegetable_10_healthBenefits" = "लाइकोपीन समृद्ध—हृदय स्वास्थ्य सुधारने वाला, सूजन कम करने वाला, और त्वचा स्वास्थ्य को बढ़ावा देने वाला।";
"vegetable_10_growingDescription" = "पौधे कुछ इंच ऊंचा हो और कम से कम 3 पत्ते हों, तब मिट्टी में गड्ढा बनाएं। पौधा लगाएँ और तुरंत पानी दें।";
"vegetable_10_harvestDescription" = "जैसे ही फल रंग बदलता है, उसे चुनें और आनंद लें।";
"vegetable_10_seedDepth" = "1/4 इंच (6 मिमी)";
"vegetable_10_watering" = "नियमित रूप से, प्रति सप्ताह 1–2 इंच";
"vegetable_10_light" = "पूरा सूर्य";
"vegetable_10_goodCompanions" = "तुलसी, लहसुन, गेंदा";
"vegetable_10_badCompanions" = "मक्का, सौंफ";

"vegetable_11_name" = "मूली";
"vegetable_11_description" = "मूली (Raphanus sativus) कुरकुरी और तीखी जड़ वाली सब्जी है, जो लाल, गुलाबी, सफेद और बैंगनी रंगों में पाई जाती है। इसे सलाद, स्लॉ और गार्निश में उपयोग किया जाता है। मूली कम कैलोरी और उच्च विटामिन C युक्त होती है जो प्रतिरक्षा और पाचन में सहायक होती है।";
"vegetable_11_healthBenefits" = "मूली फाइबर से भरपूर और कैलोरी में कम होती है, जो पाचन और जल संतुलन में मदद करती है।";
"vegetable_11_growingDescription" = "मूली की तेजी से वृद्धि के लिए नियमित और समान रूप से पानी देना आवश्यक है।";
"vegetable_11_harvestDescription" = "जब मूली की जड़ लगभग 1 इंच मोटी हो जाए, तब उसे जमीन से सीधा खींचकर निकालें।";
"vegetable_11_seedDepth" = "1/2 इंच (13 मिमी)";
"vegetable_11_watering" = "मध्यम और समान";
"vegetable_11_light" = "पूरा सूर्य";
"vegetable_11_goodCompanions" = "बीन्स, चुकंदर, गाजर";
"vegetable_11_badCompanions" = "सौंफ";

//MARK: Vegetable 13 - Arugula
"vegetable_13_name" = "अरुगुला";
"vegetable_13_description" = "अरुगुला (Eruca vesicaria), जिसे रॉकेट के नाम से भी जाना जाता है, एक पत्तेदार हरी सब्जी है जिसकी तीखी चटपटी स्वाद होती है। इसका उपयोग सलाद, सैंडविच और पिज़्ज़ा टॉपिंग में किया जाता है। यह विटामिन A, C, K और एंटीऑक्सिडेंट से भरपूर है, जो हड्डियों के स्वास्थ्य, प्रतिरक्षा और समग्र कल्याण में मदद करता है।";
"vegetable_13_healthBenefits" = "एंटीऑक्सिडेंट्स और विटामिन्स से भरपूर अरुगुला हड्डियों के स्वास्थ्य को बढ़ावा देता है और प्रतिरक्षा प्रणाली को मजबूत करता है।";
"vegetable_13_growingDescription" = "कृपया बोने के निर्देश पढ़ें।";
"vegetable_13_harvestDescription" = "पत्ते तब काटें जब वे 2 से 3 इंच लंबे हों। यह पौधों के अंकुरित होने के 2 या 3 सप्ताह बाद होता है।";
"vegetable_13_seedDepth" = "1/4 इंच (6 मिमी)";
"vegetable_13_watering" = "मध्यम और समान";
"vegetable_13_light" = "पूरा सूर्य";
"vegetable_13_goodCompanions" = "बुश बीन्स, चुकंदर, गाजर, अजवाइन, खीरा";
"vegetable_13_badCompanions" = "हरी बीन्स, स्ट्रॉबेरी";

//MARK: Vegetable 14 - Corn
"vegetable_14_name" = "मक्का";
"vegetable_14_description" = "मक्का (Zea mays), जिसे मकई भी कहा जाता है, एक बहुमुखी और व्यापक रूप से उगाई जाने वाली अनाज फसल है, जो अपने मीठे, नरम दानों के लिए जाना जाता है। इसे ताजा, ग्रिल्ड, उबला हुआ या सूप, सलाद और कैसरोल में शामिल किया जा सकता है। यह फाइबर, विटामिन B और C और ल्यूटिन जैसे एंटीऑक्सिडेंट्स से भरपूर होता है, जो स्वस्थ पाचन, प्रतिरक्षा और आंखों के स्वास्थ्य का समर्थन करता है।";
"vegetable_14_healthBenefits" = "मक्का ऊर्जा प्रदान करता है, एंटीऑक्सिडेंट्स से भरपूर है और इसके ल्यूटिन तत्व के कारण स्वस्थ दृष्टि का समर्थन करता है।";
"vegetable_14_growingDescription" = "बीजों और कतारों को सही जगह पर लगाएं। कतारों के बीच 36 इंच और बीजों के बीच भी 36 इंच की दूरी होनी चाहिए। मक्का को परागण के लिए अन्य पौधों की आवश्यकता होती है।";
"vegetable_14_harvestDescription" = "3 सप्ताह बाद दानों को छूकर जांचें। उनमें दूध जैसा तरल होना चाहिए। दाने को दबाएं, तरल बाहर निकलेगा, जो कटाई के लिए तैयार होने का संकेत है।";
"vegetable_14_seedDepth" = "1 इंच (2.5 सेमी)";
"vegetable_14_watering" = "मध्यम से भारी";
"vegetable_14_light" = "पूरा सूर्य";
"vegetable_14_goodCompanions" = "बुश बीन्स, चुकंदर, पत्ता गोभी, खीरा, मटर, लौकी";
"vegetable_14_badCompanions" = "टमाटर";



