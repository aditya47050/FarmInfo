"vegetable_21_sowIndoors" = "ट्रांसप्लांट से 4 सप्ताह पहले";
"vegetable_21_sowOutdoors" = "तैयार की गई मिट्टी पर";
"vegetable_21_phRange" = "6.5-7.0";
"vegetable_21_growingSoilTemp" = "55-65 F (13-18 C)";
"vegetable_21_spacingBeds" = "1/2\" (1.3 सेमी)";
"vegetable_21_watering" = "हल्का से मध्यम";
"vegetable_21_light" = "पूरी धूप";
"vegetable_21_goodCompanions" = "गाजर, एंडिव, आर्टिचोक";
"vegetable_21_badCompanions" = "कोई नहीं";
"vegetable_21_sowingDescription" = "लेट्यूस ठंडे मौसम की सब्जी है। नर्सरी से पौधे भी खरीदे जा सकते हैं। वसंत में बीज बोना सबसे अच्छा होता है। ऐसी जगह चुनें जहाँ ठंडी धूप मिलती हो।";
"vegetable_21_growingDescription" = "लेट्यूस की किस्म पर निर्भर करते हुए पौधों के बीच की दूरी रखें। मिट्टी को नम रखें पर अधिक पानी से बचें। अत्यधिक नमी से फफूंदी या जड़ों का सड़ना हो सकता है।";
"vegetable_21_harvestDescription" = "जब पत्तियाँ पर्याप्त बड़ी हो जाएँ, तो उन्हें काट सकते हैं। कटाई तब करें जब सिर मजबूत और पूरी तरह विकसित हो।";
"vegetable_21_season" = "पतझड़, वसंत, गर्मी, सर्दी";
"vegetable_21_daysToHarvestSeeds" = "75 दिन";
"vegetable_21_daysToHarvestSeedlings" = "60 दिन";
"vegetable_21_healthBenefits" = "लेट्यूस में कैलोरी कम और विटामिन अधिक होते हैं, जो हाइड्रेशन और स्वास्थ्य को समर्थन देते हैं।";

//MARK: Pests for Lettuce (vegetable_21)

"pest_Aphids_name" = "एफिड्स";
"pest_Aphids_body" = "एफिड्स छोटे, मुलायम कीड़े होते हैं जो पत्तियों, तनों और कलियों से रस चूसते हैं। ये विभिन्न रंगों में पाए जाते हैं और जल्दी बढ़ते हैं।";
"pest_Aphids_symptoms" = "पत्तियाँ पीली, मुड़ी हुई और चिपचिपी हो सकती हैं। हनीड्यू और काली फफूंदी दिख सकती है।";
"pest_Aphids_treatment" = "लेडीबग्स जैसे लाभकारी कीटों को छोड़ें, पानी का तेज स्प्रे करें या नीम ऑयल लगाएँ।";

"pest_Leaf Miners_name" = "लीफ माइनर्स";
"pest_Leaf Miners_body" = "लीफ माइनर्स विभिन्न कीटों के लार्वा होते हैं जो पत्तियों के बीच की परतों को खाते हैं, जिससे सफेद सुरंगें बनती हैं।";
"pest_Leaf Miners_symptoms" = "पत्तियों पर सफेद लकीरें या सुरंगें दिखती हैं जो समय के साथ भूरे पड़ जाती हैं।";
"pest_Leaf Miners_treatment" = "संक्रमित पत्तियों को हटाएं, नीम ऑयल का प्रयोग करें और परजीवी ततैया जैसे कीटों को बढ़ावा दें।";

//MARK: Vegetables - Cauliflower

"vegetable_22_name" = "फूलगोभी";
"vegetable_22_description" = "फूलगोभी (Brassica oleracea var. botrytis) एक क्रूसीफेरस सब्जी है जो सफेद कलियों और हल्के स्वाद के लिए जानी जाती है। यह भूनकर, उबालकर, मैश कर या चावल/पिज़्ज़ा के विकल्प के रूप में खाई जाती है। इसमें विटामिन C, K, फाइबर और एंटीऑक्सीडेंट प्रचुर होते हैं।";
"vegetable_22_seedDepth" = "1/4-1/2\" (6-13 मिमी)";
